config_version=5

[application]

config/name="spine-viewer"
config/description="spine-viewer"
config/version="0.1.1"
run/main_scene="res://scene/main.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
run/max_fps=260
boot_splash/show_image=false
config/icon="res://icon.png"
config/macos_native_icon="res://icon.icns"
config/windows_native_icon="res://icon.ico"

[autoload]

SpriteLoader="*res://script/SpriteLoader.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/transparent=true
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="spine-viewer"

[gui]

theme/default_font_generate_mipmaps=true
theme/custom_font="res://assets/unifont.otf"

